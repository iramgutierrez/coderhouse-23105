<%= message.join(", "); %>
<form action="/login" method="post">
    <div>
        <label>Nombre de usuario:</label>
        <input type="text" name="username"/>
    </div>
    <div>
        <label>Contraseña:</label>
        <input type="password" name="password"/>
    </div>
    <div>
        <input type="submit" value="Iniciar sesión"/>
    </div>
</form>